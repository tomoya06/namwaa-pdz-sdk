  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test All",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "test"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
  {
    "name": "Test One Script",
    "request": "launch",
    "runtimeArgs": [
      "run",
      "testone"
    ],
    "runtimeExecutable": "npm",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "pwa-node"
  },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}